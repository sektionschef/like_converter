	//		console.log(count + " größer als " + count_minus);
			var diff = count-count_minus
			for (z=1; z<=diff; z++) 
				console.log("I schiaß a " + z + ". moi");
		} else {
	//		console.log(count + " kleiner gleich " + count_minus);
		}
	//}

}, null, true); //keine ahnung was das null und das true machen.
	
